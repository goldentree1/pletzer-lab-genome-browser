{
  "name": "vite-react-lgv",
  "private": true,
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "concurrently -k -n site,data \"vite --port 5173\" \"npx serve -S ./public/data -p 5174\"",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "aws s3 sync --delete dist s3://jbrowse.org/demos/lgv-vite/",
    "postdeploy": "AWS_PAGER=\"\" aws cloudfront  create-invalidation --distribution-id E13LGELJOT4GQO --paths \"/demos/lgv-vite/*\"",
    "lint": "eslint ."
  },
  "dependencies": {
    "@fontsource/roboto": "^5.1.0",
    "@jbrowse/core": "^3.0.0",
    "@jbrowse/react-linear-genome-view2": "^3.0.0",
    "buffer": "^6.0.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/express": "^5.0.6",
    "@types/node": "^20.3.3",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.3",
    "concurrently": "^9.2.1",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "express": "^5.2.1",
    "globals": "^16.0.0",
    "prettier": "^3.3.3",
    "tsx": "^4.21.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^6.0.3"
  }
}
